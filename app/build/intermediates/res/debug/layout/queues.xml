<?xml version="1.0" encoding="utf-8"?>
<FrameLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/queueFrag"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:padding="10dp"
            android:background="@drawable/login_fieldset"
            android:orientation="vertical"
            android:id="@+id/yetContent">
    <TextView
        android:id="@+id/citRegHeader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/update_header"
        android:text="@string/addCitizenHeader"/>
    <TextView
        android:id="@+id/lostid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/fonts"
        android:visibility="gone"/>
    <TextView
        android:id="@+id/tvOwner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/owner"
        style="@style/fonts"
        android:background="@drawable/textviews"/>

    <EditText
        android:id="@+id/edtOwner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="5dp"
        android:singleLine="true"
        android:drawableLeft="@mipmap/loguser"
        android:background="@drawable/edittexts"></EditText>
            <TextView
                android:id="@+id/tvNid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/nid"
                style="@style/fonts"
                android:background="@drawable/textviews"/>
            <EditText
                android:id="@+id/edtNid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:singleLine="true"
                android:drawableLeft="@mipmap/pay_card_blue"
                android:background="@drawable/edittexts"
                android:inputType="number"
                android:maxLength="16"></EditText>

    <TextView
        android:id="@+id/tvUname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/officetel"
        style="@style/fonts"
        android:background="@drawable/textviews"/>

    <EditText
        android:id="@+id/edtPhone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="5dp"
        android:singleLine="true"
        android:drawableLeft="@mipmap/phonenbr"
        android:background="@drawable/edittexts"
        android:inputType="number"
        android:maxLength="10"></EditText>

            <TextView
                android:id="@+id/readyRegistered"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/update_header"
                android:text="@string/readyregistered"/>
    <!--TextView
        android:id="@+id/tvProv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/releaseProv"
        style="@style/fonts"/>
    <AutoCompleteTextView
        android:id="@+id/autoProv"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        ></AutoCompleteTextView-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/layout_btn">
        <Button
            android:id="@+id/btnRegQueue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/addnew_btnsave"
            style="@style/btn"
            android:background="@drawable/buttons"
            />
    </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:padding="10dp"
            android:background="@drawable/login_fieldset"
            android:orientation="vertical"
            android:id="@+id/readyContent"
            android:visibility="gone">
            <TextView
                android:id="@+id/queuHeader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/update_header"
                android:text="@string/qHeader"/>
            <TextView
                android:id="@+id/tvCitname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/tvOwner"
                style="@style/fonts"
                android:background="@drawable/textviews"
                android:visibility="gone"/>
            <EditText
                android:id="@+id/edtCitname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:singleLine="true"
                android:drawableLeft="@mipmap/phone"
                android:background="@drawable/edittexts"
                android:editable="false"
                android:visibility="gone"></EditText>
            <TextView
                android:id="@+id/tvNiD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/nid"
                style="@style/fonts"
                android:background="@drawable/textviews"/>
            <EditText
                android:id="@+id/edtNiD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:singleLine="true"
                android:drawableLeft="@mipmap/pay_card_blue"
                android:background="@drawable/edittexts"
                android:inputType="number"
                android:maxLength="16"></EditText>

            <TextView
                android:id="@+id/tvIdentification"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/identification"
                style="@style/fonts"
                android:background="@drawable/textviews"/>

            <EditText
                android:id="@+id/edtIdentification"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:singleLine="true"
                android:drawableLeft="@mipmap/id"
                android:background="@drawable/edittexts"></EditText>
            <TextView
                android:id="@+id/tvIdType"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/type"
                style="@style/fonts"
                android:background="@drawable/textviews"/>
            <Spinner
                android:id="@+id/spnType"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:background="@drawable/spinners"
                ></Spinner>
            <TextView
                android:id="@+id/yetRegistered"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                style="@style/update_header"
                android:text="@string/notyetregistered"/>
            <!--TextView
                android:id="@+id/tvProv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/releaseProv"
                style="@style/fonts"/>
            <AutoCompleteTextView
                android:id="@+id/autoProv"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                ></AutoCompleteTextView-->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:id="@+id/layout_btnreg_item">
                <Button
                    android:id="@+id/btnRegItem"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/addnew_btnsave"
                    style="@style/btn"
                    android:background="@drawable/buttons"
                    />
            </LinearLayout>
        </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
